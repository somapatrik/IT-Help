<Application x:Class="IT_Help.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:IT_Help"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Colors.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <Style x:Key="redgrid" TargetType="Grid">
                <Setter Property="Background" Value="{StaticResource Gray100Brush}"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>

</Application>
